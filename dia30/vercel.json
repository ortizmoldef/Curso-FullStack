{
    "version": 2,
    "builds": [
      {
        "src": "server.js",  // Usa 'server.js' como el archivo de entrada
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",  // Redirige todas las rutas a 'server.js'
        "dest": "server.js"
      }
    ]
  }
  